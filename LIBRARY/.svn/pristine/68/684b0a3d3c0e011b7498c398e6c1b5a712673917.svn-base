package com.kh.library.book.vo;

public class RecommendImgVO {
	private String rcImgCode;
	private String rcOriginName;
	private String rcAtImgName;
	private String rcCode;
	
	public String getRcImgCode() {
		return rcImgCode;
	}
	public void setRcImgCode(String rcImgCode) {
		this.rcImgCode = rcImgCode;
	}
	public String getRcOriginName() {
		return rcOriginName;
	}
	public void setRcOriginName(String rcOriginName) {
		this.rcOriginName = rcOriginName;
	}
	public String getRcAtImgName() {
		return rcAtImgName;
	}
	public void setRcAtImgName(String rcAtImgName) {
		this.rcAtImgName = rcAtImgName;
	}
	public String getRcCode() {
		return rcCode;
	}
	public void setRcCode(String rcCode) {
		this.rcCode = rcCode;
	}
	
	
}
